{
  "name": "adonisjs-kwikng",
  "version": "0.0.1",
  "description": "An addon/plugin package to provide KwikNG automated last-mile delivery services in AdonisJS 4.1+",
  "main": "index.js",
  "files": [
    "src",
    "config",
    "providers",
    "instructions.js",
    "instructions.md"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "standard --fix",
    "pretest": "npm run lint",
    "test": "nyc node japaFile.js",
    "test:win": "node ./node_modules/japa-cli/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "last-mile delivery",
    "automation",
    "adonisjs",
    "kwik",
    "delivery",
    "APIs",
    "adonis-framework",
    "logistics",
    "adonisjs-addon"
  ],
  "author": "<StitchNG> https://github.com/stitchng",
  "license": "MIT",
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "standard": {
    "globals": [
      "use",
      "test"
    ]
  },
  "dependencies": {
    "@adonisjs/generic-exceptions": "^2.0.1",
    "kwik-node": "^0.0.2",
    "debug": "^3.1.2"
  },
  "devDependencies": {
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/sink": "^1.0.6",
    "coveralls": "^3.0.2",
    "japa": "^2.0.5",
    "japa-cli": "^1.0.1",
    "nyc": "^13.0.1",
    "semver": "^5.5.0",
    "standard": "^12.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stitchng/adonis-kwikng.git"
  },
  "bugs": {
    "url": "https://github.com/stitchng/adonis-kwikng/issues"
  },
  "homepage": "https://github.com/stitchng/adonis-kwikng#readme"
}
